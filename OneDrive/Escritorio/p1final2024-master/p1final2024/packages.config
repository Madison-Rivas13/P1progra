<?xml version="1.0" encoding="utf-8"?>
<!-- Este archivo contiene las referencias de paquetes NuGet utilizados en el proyecto. -->
<packages>
	<!-- BouncyCastle para operaciones criptográficas -->
	<package id="BouncyCastle.Cryptography" version="2.2.1" targetFramework="net472" />

	<!-- Google Protobuf para serialización y deserialización de mensajes -->
	<package id="Google.Protobuf" version="3.25.1" targetFramework="net472" />

	<!-- K4os LZ4 para compresión rápida de datos -->
	<package id="K4os.Compression.LZ4" version="1.3.5" targetFramework="net472" />
	<package id="K4os.Compression.LZ4.Streams" version="1.3.5" targetFramework="net472" />

	<!-- K4os Hash para algoritmos de hash rápidos -->
	<package id="K4os.Hash.xxHash" version="1.0.8" targetFramework="net472" />

	<!-- Microsoft BCL Async Interfaces para soporte de tareas asincrónicas -->
	<package id="Microsoft.Bcl.AsyncInterfaces" version="5.0.0" targetFramework="net472" />

	<!-- MySQL Data para conectividad con bases de datos MySQL -->
	<package id="MySql.Data" version="8.4.0" targetFramework="net472" />

	<!-- System.Buffers para operaciones con buffers -->
	<package id="System.Buffers" version="4.5.1" targetFramework="net472" />

	<!-- Configuration Manager para manejo de configuraciones -->
	<package id="System.Configuration.ConfigurationManager" version="4.4.1" targetFramework="net472" />

	<!-- DiagnosticSource para diagnósticos y monitoreo -->
	<package id="System.Diagnostics.DiagnosticSource" version="7.0.2" targetFramework="net472" />

	<!-- Pipelines para operaciones de E/S de alto rendimiento -->
	<package id="System.IO.Pipelines" version="5.0.2" targetFramework="net472" />

	<!-- Memory para operaciones con memoria eficiente -->
	<package id="System.Memory" version="4.5.5" targetFramework="net472" />

	<!-- Numerics Vectors para operaciones matemáticas con vectores -->
	<package id="System.Numerics.Vectors" version="4.5.0" targetFramework="net472" />

	<!-- Runtime Compiler Services Unsafe para operaciones inseguras -->
	<package id="System.Runtime.CompilerServices.Unsafe" version="6.0.0" targetFramework="net472" />

	<!-- Tasks Extensions para tareas extendidas -->
	<package id="System.Threading.Tasks.Extensions" version="4.5.4" targetFramework="net472" />

	<!-- ZstdSharp para compresión rápida usando Zstandard -->
	<package id="ZstdSharp.Port" version="0.7.1" targetFramework="net472" />
</packages>
